{
  "manifest_version": 2,
  
  "name": "Genie Extension",
  "description": "An extension to put genie's lamp in every website you visit.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "jquery.js",
        "angular.js",
        "genie.js",
        "uxGenie.js",
        "extension.js"
      ],
      "css": ["styles.css"]
    }
  ]
}