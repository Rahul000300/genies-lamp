{
  "manifest_version": 2,
  
  "name": "Genie's Lamp",
  "short_name": "Genie's Lamp",
  "description": "An extension to put genie's lamp in every website you visit.",
  "version": "0.0.1",
  
  "author": "Kent C. Dodds <kent@doddsfamily.us>",
  
  "homepage_url": "https://github.com/kentcdodds/genies-lamp",
  
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "jquery.js",
        "pre-load.js",
        "angular.js",
        "genie.js",
        "uxGenie.js",
        "extension.js"
      ],
      "css": ["styles.css"]
    }
  ]
}